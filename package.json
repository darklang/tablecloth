{
  "name": "tablecloth-rescript",
  "description": "A portable standard library enhancement for Rescript and OCaml.",
  "version": "0.0.8",
  "repository": "https://github.com/darklang/tablecloth",
  "private": false,
  "contributors": [
    "Paul Biggar <paul@darklang.com>"
  ],
  "keywords": [
    "Rescript",
    "OCaml",
    "BuckleScript",
    "ReasonML",
    "Utility"
  ],
  "license": "MIT",
  "devDependencies": {
    "@glennsl/bs-jest": "0.7.0",
    "rescript": "10.0.1"
  },
  "scripts": {
    "clean": "rescript clean",
    "build": "rescript build -with-deps",
    "test": "jest",
    "build:watch": "rescript build -with-deps -w"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/rescript/test/*Test.bs.js",
      "<rootDir>/rescript/test/*Test.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/rescript/node_modules/"
    ]
  }
}
